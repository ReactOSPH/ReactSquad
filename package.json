{
  "name": "ReactSquad",
  "version": "2.0.1",
  "description": "ReactSquad, now with a brand new look!",
  "main": "scripts.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && run-p build:*",
    "build:action": "rm -rf dist && esbuild scripts.js --bundle --platform=node --target=node16 --sourcemap=external --outdir=dist",
    "build:site": "siteup --src . --dest public",
    "watch": "npm run clean && run-p watch:*",
    "watch:serve": "browser-sync start --server 'public' --files 'public'",
    "watch:site": "npm run build:site -- -w",
    "clean": "rm -rf public && mkdir -p public",
    "start": "npm run watch"
  },
  "keywords": [],
  "author": "ReactSquad",
  "license": "MIT",
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "async-neocities": "2.1.5",
    "minimatch": "9.0.3",
    "ms": "2.1.3",
    "pony-cause": "^2.1.4",
    "webassert": "3.0.2"
  },
  "devDependencies": {
    "@siteup/cli": "^2.0.0",
    "auto-changelog": "^2.0.0",
    "browser-sync": "^2.27.7",
    "dependency-check": "^4.1.0",
    "gh-release": "^7.0.0",
    "npm-run-all2": "^6.0.0",
    "standard": "^17.0.0",
    "esbuild": "^0.18.0"
  }
}
