{
  "name": "reactsquad.neocities.org",
  "version": "2.0.1",
  "description": "The home of ReactOS-PH and ReactOS Italia!",
  "main": "staging/scripts.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && run-p build:*",
    "build:css": "postcss src/style.css -o staging/style.css",
    "build:js": "browserify staging/scripts.js --debug | exorcist staging/scripts.js.map > staging/scripts.js",
    "build:static": "cpx 'staging/**/*.*' staging",
    "watch": "npm run clean && run-p watch:*",
    "watch:css": "run-s 'build:css -- --watch'",
    "watch:js": "budo staging/scripts.js:scripts.js --dir staging --live --open",
    "watch:static": "npm run build:static -- --watch --verbose",
    "clean": "rimraf staging && mkdirp staging",
    "start": "npm run watch"
  },
  "homepage": "https://reactsquad.neocities.org",
  "keywords": [],
  "author": "ReactSquad",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ReactOSPH/ReactSquad.git"
  },
  "bugs": {
    "url": "https://github.com/ReactOSPH/ReactSquad/issues"
  },
  "devDependencies": {
    "@tap-format/spec": "^0.2.0",
    "autoprefixer": "^9.7.4",
    "browserify": "^16.5.0",
    "budo": "^11.6.3",
    "cpx2": "^2.0.0",
    "dependency-check": "^4.1.0",
    "exorcist": "^1.0.1",
    "mkdirp": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.26",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "rimraf": "^3.0.0",
    "snazzy": "^8.0.0",
    "standard": "^14.3.1",
    "tape": "^5.0.0",
    "mine.css": "^2.0.0"
  },
  "engines": {
    "node": "*",
    "vscode": "*",
    "npm": "*"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "_comment": "most code is from ecomnes/elenacomnes.com"
}